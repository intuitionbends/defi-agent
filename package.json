{
  "name": "defi-agent",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "npx @langchain/langgraph-cli dev --port 2024 --config ./langgraph.json",
    "build": "turbo build:internal",
    "build:internal": "npm run clean && tsc",
    "version": "echo $(git rev-list --count HEAD)-$(git rev-parse --short HEAD)",
    "clean": "rm -rf dist",
    "dev:start": "ts-node src/main.ts",
    "start": "tsc && node dist/main.js",
    "migrate:up": "npx db-migrate up",
    "migrate:create": "npx db-migrate create",
    "migrate:down": "npx db-migrate down -c",
    "migrate:clean": "npx db-migrate down -c 100",
    "docker:build": "docker build -t defi-agent:$(npm run version -s) -f Dockerfile .",
    "docker:push": "docker tag defi-agent:$(npm run version -s) 674734398360.dkr.ecr.ap-southeast-1.amazonaws.com/delaunch/defi-agent:$(npm run version -s) && docker push 674734398360.dkr.ecr.ap-southeast-1.amazonaws.com/delaunch/defi-agent:$(npm run version -s)",
    "docker:build_push": "npm run docker:build && npm run docker:push"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.1",
    "@langchain/anthropic": "^0.3.15",
    "@langchain/cohere": "^0.3.2",
    "@langchain/mongodb": "^0.1.0",
    "@langchain/openai": "^0.4.4",
    "@langchain/pinecone": "^0.2.0",
    "@pinecone-database/pinecone": "^5.1.1",
    "mongodb": "^6.14.2",
    "tavily": "^1.0.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "@intuition-bends/common-js": "^1.2.0",
    "@jest/globals": "^29.7.0",
    "@langchain/community": "^0.3.41",
    "@langchain/core": "^0.3.43",
    "@langchain/langgraph": "^0.2.63",
    "@supabase/supabase-js": "^2.49.1",
    "axios": "^1.8.4",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.4.7",
    "ethers": "^5.8.0",
    "express": "^4.21.2",
    "langchain": "^0.3.20",
    "pg": "^8.14.1",
    "winston": "^3.17.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.14",
    "@types/pg": "^8.11.11",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^17.0.33",
    "supabase": "^2.15.8",
    "ts-node": "^10.9.2",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@jest/globals": "^29.7.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "turbo": "latest",
    "typescript": "^5"
  }
